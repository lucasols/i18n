{
  "name": "@ls-stack/i18n-monorepo",
  "private": true,
  "description": "Super simple i18n library monorepo",
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "test": "vitest --ui",
    "test:run": "vitest run",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "pnpm -r tsc",
    "eslint": "CI=true eslint packages/*/src/ --color --max-warnings=0",
    "build": "pnpm test:run && pnpm lint && pnpm build:no-test",
    "build:no-test": "pnpm -r build:no-test",
    "pre-publish": "./scripts/check-if-is-sync.sh && pnpm build"
  },
  "repository": "https://github.com/lucasols/i18n",
  "author": "Lucas Santos",
  "type": "module",
  "engines": {
    "node": ">=21.5.0"
  },
  "devDependencies": {
    "@ls-stack/extended-lint": "^0.11.2",
    "@ls-stack/utils": "^3.3.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.14.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
